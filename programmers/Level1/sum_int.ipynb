   "source": [
    "def solution(a, b):\n",
    "    answer = 0\n",
    "    if a < b:\n",
    "        for i in range(a, b+1):\n",
    "            answer += i\n",
    "    else:\n",
    "        for i in range(b, a+1):\n",
    "            answer += i\n",
    "    return answer\n",
    "\n",
    "a = 3\n",
    "b = 5\n",
    "solution(a, b)"
   ]
  }
