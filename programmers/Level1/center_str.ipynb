    "def solution(s):\n",
    "    if len(s) % 2 == 0:\n",
    "        index = int(len(s) / 2)\n",
    "        answer = s[index-1]\n",
    "        answer += s[index]\n",
    "    else:\n",
    "        index = int(len(s)/2)\n",
    "        answer = s[index]\n",
    "    return answer\n",
    "\n",
    "s = 'abcde'\n",
    "solution(s)\n",
