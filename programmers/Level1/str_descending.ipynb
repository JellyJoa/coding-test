    "def solution(s):\n",
    "    lst = []\n",
    "    answer = ''\n",
    "    for i in s:\n",
    "        i = ord(i)\n",
    "        lst.append(i)\n",
    "    lst.sort(reverse=True)\n",
    "    for j in lst:\n",
    "        answer += chr(j)\n",
    "    return answer\n",
    "\n",
    "s = 'Zbcdefg'\n",
    "solution(s)"
   ]
